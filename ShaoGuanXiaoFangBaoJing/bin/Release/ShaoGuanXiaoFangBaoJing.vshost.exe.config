<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /> 
    </startup>
  <appSettings>
    <add key="IP" value="127.0.0.1"/><!--服务器IP-->
    <add key="SlaveID" value="0"/><!--默认0-->
    <add key="StartAddress" value="0"/><!--默认起始为0-->
    <add key="PointNumber" value="10"/><!--每次读取的结果数，不能超过125。如设置为10，表示每次读取10*16个设备的状态-->
    <add key="MaxPoint" value="100"/><!--读取的最大结果数。如设置为1000，表示最多读取1000*16个设备的状态.建议设置为PointNumber的整数倍-->
    <add key="kafkaUrl" value="10.130.42.8:9092,10.130.42.9:9092,10.130.42.10:9092"/>
    <add key="topicAlarm" value="cContainerAlarm"/>
    <add key="functionType" value="Hold"/><!--Hold/Input-->
    <add key="scanTimeSpan" value="60"/><!--扫描的时间间隔，单位秒-->
  </appSettings>
</configuration>